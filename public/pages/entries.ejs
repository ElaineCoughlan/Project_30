<div class="jumbotron text-center">
	<h1>Parts asked?</h1>
	
	<p>{{ message }}</p>

  <div ng-controller="entriesController as list">
	<table align="center" valign="middle">
	  <tbody ng-repeat="entry in entries | orderBy:'-Likey'">
        <tr style="height:55px; font-size:20px; margin-left:20px; margin-right:20px; valign:middle"; >
        	<td>
            <span class="glyphicon glyphicon-thumbs-up" ng-click="incrementUpvotes(donation._id)"></span>
              {{entry.Likey}}
             </td>

            <td>
                <span class="glyphicon glyphicon-thumbs-down" ng-click="incrementUpvotes(donation._id)"></span>
                {{entry.NoLikey}}
            </td>

             <td> 
             <span style="height:55px; font-size:20px; margin-left:20px; margin-right:20px; valign:middle";>
                 {{entry.NewPart}}</span>

         	</td>

            <td>
             <span style="height:55px; font-size:20px; margin-left:20px; margin-right:20px; valign:middle";>
                 {{entry.OldPart}}</span>

            </td>

            <td>
             <span style="height:55px; font-size:20px; margin-left:20px; margin-right:20px; valign:middle";>
                 {{entry.Device}}</span>

            </td>

 			<td>
      
      <span style="height:55px; font-size:20px; margin-left:20px; margin-right:20px; valign: middle";>

      
      <button type="button" class="btn btn-default" ng-click="delete(entry._id)">
          <span class="glyphicon glyphicon-remove"></span> Remove 
        </button>
        </span>    
             </td>
    	</tr>
      </tbody>
    </table> 
  </div>
</div>